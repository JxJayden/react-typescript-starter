{
  "name": "react-typescript-starter",
  "version": "1.0.0",
  "description": "A simple React-Typescript Template",
  "scripts": {
    "start": "node ./build/dev-server.js",
    "dev": "node ./build/dev-server.js",
    "build": "node ./build/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,scss,css,less}": ["./node_modules/.bin/tslint --fix", "prettier-eslint --write", "git add"],
    "src/**/*.{ts,tsx}": ["./node_modules/.bin/tslint --fix", "git add"]
  },
  "keywords": ["React", "TypeScript"],
  "author": "JxJayden <jxdjayden@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.0.13",
    "@types/react-dom": "^16.0.1",
    "babel-polyfill": "^6.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.3.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prettier-eslint": "^8.2.1",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.0.2",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-parallel-uglify-plugin": "^1.0.0",
    "webpack-simple-progress-plugin": "^0.0.2"
  }
}
